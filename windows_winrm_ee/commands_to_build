ssh a nodo controlador bcegansbl01.bce.fin.ec --> nodo controlador

1. mkdir ee-winrm
2. git clone del repo 
3. rm -rf context/

4. vi token.txt ---> copiar el token del console.redhat.com -> ingresar con credenciales de redhat -> connect to hub --> copiar token al archivo token.txt
5. export ANSIBLE_GALAXY_SERVER_AUTOMATION_HUB_TOKEN=$(cat token.txt)
6. ansible-builder build -v 3 --build-arg ANSIBLE_GALAXY_SERVER_AUTOMATION_HUB_TOKEN -t windows-ee:v1.0

